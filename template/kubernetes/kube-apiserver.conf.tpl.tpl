KUBE_LOG="--logtostderr=true"
KUBE_LOG_VER="--v=4"
KUBE_ETCD_SERVER="--etcd-servers=https://$1:2379"
KUBE_ADDRESS="--bind-address=$1"
KUBE_SECURE_PORT="--secure-port=6443"
KUBE_ADVERTISE_ADDRESS="--advertise-address=$1"
KUBE_PRIVILEGED="--allow-privileged=true"
KUBE_CLUSTER_IP_RANGE="--service-cluster-ip-range=10.0.0.0/16"
KUBE_ADMINSSION_PLUGINS="--enable-admission-plugins=NamespaceLifecycle,LimitRanger,ServiceAccount,ResourceQuota,NodeRestriction"
KUBE_AUTHORIZATION_MOD="--authorization-mode=RBAC,Node"
KUBE_BOOTSTRAP="--enable-bootstrap-token-auth"
KUBE_TOKEN_AUTH_FILE="--token-auth-file=/opt/kubernetes/cfg/token.csv"
KUBE_SERVICE_NODE_PORT="--service-node-port-range=30000-50000"
KUBE_TLS_CERT_FILE="--tls-cert-file=/opt/kubernetes/ssl/apiserver.pem"
KUBE_TLS_PRIVATE_KEY="--tls-private-key-file=/opt/kubernetes/ssl/apiserver-key.pem"
KUBE_CLIENT_CA_FILE="--client-ca-file=/opt/kubernetes/ssl/ca.pem"
KUBE_SERVICE_ACCOUNT_KEY="--service-account-key-file=/opt/kubernetes/ssl/ca-key.pem"
KUBE_KUBELET_HTTPS="--kubelet-https=true"
KUBE_KUBELET_CERTIFICATE-AUTHORITY="--kubelet-certificate-authority=ca.pem"
KUBE_KUBELET_CLIENT_CERT="--kubelet-client-certificate=/opt/kubernetes/ssl/admin.pem"
KUBE_KUBELET_CLIENT_KEY="--kubelet-client-key=/opt/kubernetes/ssl/admin-key.pem"
KUBE_ETCD_CA="--etcd-cafile=/opt/etcd/ssl/ca.pem"
KUBE_ETCD_CERT="--etcd-certfile=/opt/etcd/ssl/server.pem"
KUBE_ETCD_KEY="--etcd-keyfile=/opt/etcd/ssl/server-key.pem"
KUBE_PROXY_CLIENT_CERT="--proxy-client-cert-file=/opt/kubernetes/ssl/metrics-server.pem"
KUBE_PROXY_CLIENT_KEY_FILE="--proxy-client-key-file=/opt/kubernetes/ssl/metrics-server-key.pem"
KUBE_REQUEST_HEADER="--requestheader-allowed-names=aggregator"
KUBE_REQUESTHEADER_CLIENT_CA_FILE="--requestheader-client-ca-file=/opt/kubernetes/ssl/ca.pem"
KUBE_REQUESTHEADER_EXTRA_HEADERS_PREFIX="--requestheader-extra-headers-prefix=X-Remote-Extra-"
KUBE_REQUESTHEADER_GROUP_HEADERS="--requestheader-group-headers=X-Remote-Group"
KUBE_REQUESTHEADER_USERNAME_HEADERS="--requestheader-username-headers=X-Remote-User"
KUBE_ENABLE_AGREGATOR-ROUTING="--enable-aggregator-routing=true"